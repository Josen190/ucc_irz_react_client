{
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 80,
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "parser": "javascript",
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "es5",
        "importOrder": ["^@.*$", "^\\u0000", "^.*$"],
        "importOrderSeparation": true
      }
    }
  ]
}
